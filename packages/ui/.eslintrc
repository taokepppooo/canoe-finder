{
  "root": true,
  "extends": [
    "eslint:recommended",
    "prettier",
    "plugin:@typescript-eslint/recommended",
    "plugin:prettier/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": ["prettier", "@typescript-eslint"],
  "rules": {
    "@typescript-eslint/no-explicit-any": "off",
    "comma-dangle": "off",
    "prettier/prettier": ["error", {
      "trailingComma": "all",
      "semi": true
    }]
  },
  "parserOptions": {
    "ecmaVersion":"latest",
    "tsconfigRootDir": ".",
    "sourceType": "module"
  }
}
